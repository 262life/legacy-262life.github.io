ssh_authorized_keys:
- ssh-rsa AAAAB3NzaC1yc2EAAAADAQAB...
- github:rsliotta
write_files:
- path: /var/lib/connman/default.config
  encoding: ""
  owner: root
  permissions: '0755'
  content: |-
    [service_eno1]
    Type=ethernet
    IPv4=192.168.1.252/255.255.255.0/192.168.1.1
    IPv6=off
    Nameservers=8.8.8.8,8.8.4.4
hostname: rome
k3os:
  data_sources:
  - aws
  - cdrom
  modules:
  - kvm
  - nvme
  sysctl:
    kernel.printk: "4 4 1 7"
    kernel.kptr_restrict: "1"
  dns_nameservers:
  - 8.8.8.8
  - 1.1.1.1
  ntp_servers:
  - 0.us.pool.ntp.org
  - 1.us.pool.ntp.org
  wifi:
  - name: home
    passphrase: mypassword
  - name: nothome
    passphrase: somethingelse
  password: rancher
  server_url: https://127.0.0.1:6443
  token: TOKEN_VALUE
  labels:
    region: us-west-1
    somekey: somevalue
  k3s_args:
  - server
  # - "--disable-agent "
  #environment:
  #  http_proxy: http://myserver
  #  https_proxy: http://myserver
  #taints:
  #- key1=value1:NoSchedule
  #- key1=value1:NoExecute
